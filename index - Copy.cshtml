            randombar.classList.toggle("opacity");
            but.classList.toggle("butt");
            if (rnd.classList.contains('opacity')) { but.innerHTML = 'close' }
            else { but.innerHTML = 'random' }

        });

        var a = document.getElementById('asa');
        var str = document.getElementById('strongzone');
        var st = str.cloneNode(true);
        var wea = document.getElementById('weakzone');
        var we = wea.cloneNode(true);
        var ch = document.getElementById('checkzone');
        var c = ch.cloneNode(true);

        if (window.innerWidth <= 700) {
            a.appendChild(st);
            a.appendChild(we);
            a.appendChild(c)
            str.parentNode.removeChild(str);
            wea.parentNode.removeChild(wea);
            ch.parentNode.removeChild(ch);
        }

    </script>

    @if (IsPost)
    {
        string teamname = Request["teamname"];
        string teamtier = Request["teamtier"];
        string teamtext = Request["teamtext"];
        if (Request["captcha"] != null)
        {
            if (Request["captcha"] != "")
            {
                SmtpClient client = new SmtpClient();
                client.Host = "smtp.live.com";
                client.Port = 587;
                client.UseDefaultCredentials = false;
                client.DeliveryMethod = SmtpDeliveryMethod.Network;
                client.EnableSsl = true;
                client.Credentials = new NetworkCredential(ConfigurationManager.AppSettings["emailname"], ConfigurationManager.AppSettings["emailpass"]);
                string mailtext = "Raw:\n\n" + teamtext + "\n\n" + "Importable" + "\n\n" +
                    '"' + teamname + '"' + ": {" + "\n" + "name: " + '"' + teamname + '"' + "," + "\n" +
                    "tier: " + '"' + teamtier + '"' + "," + "\n" + "data: " + '"' + teamtext.Replace("\r\n","\\n\\\n")  + "\\n" + '"' + "\n" + "}";
                client.Send(new MailMessage(ConfigurationManager.AppSettings["emailname"], ConfigurationManager.AppSettings["emailname"],'"' + FirstCharToUpper(teamname) + '"' + " (" + teamtier + ")", teamtext));
                Response.Write("<script>alert('Thank you! Your team has been submitted for review.');</script>");
            }
            else
            {
                Response.Write("<script>alert('Error! Please complete the Captcha before submitting!');</script>");
            }
        }
        else
        {
            Response.Write("<script>alert('Error! Please complete the Captcha before submitting!');</script>");
        }

    }

