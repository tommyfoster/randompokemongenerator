@using System.Net.Mail;
@using System.Configuration;

@functions {
    public static string FirstCharToUpper(string value)
    {
        char[] array = value.ToCharArray();
        // Handle the first letter in the string.  
        if (array.Length >= 1)
        {
            if (char.IsLower(array[0]))
            {
                array[0] = char.ToUpper(array[0]);
            }
        }
        // Scan through the letters, checking for spaces.  
        // ... Uppercase the lowercase letters following spaces.  
        for (int i = 1; i < array.Length; i++)
        {
            if (array[i - 1] == ' ')
            {
                if (char.IsLower(array[i]))
                {
                    array[i] = char.ToUpper(array[i]);
                }
            }
        }
        return new string(array);
    }
}

@{
    string rawdata = "";
    bool islive = false;
    HttpWebRequest request = (HttpWebRequest)WebRequest.Create("https://api.twitch.tv/kraken/streams/pyrotoz");
    request.Headers.Add("Client-ID", ConfigurationManager.AppSettings["apikey"]);
    request.ContentType = "application/json";

    using(HttpWebResponse response = (HttpWebResponse)request.GetResponse())
    {
        using (Stream stream = response.GetResponseStream())
        {
            using (StreamReader reader = new StreamReader(stream))
            {
                rawdata =  reader.ReadToEnd();
            }

        }

    }

    var jsondata = Json.Decode(rawdata);
    if (jsondata.stream != null) { islive = true; }


}

